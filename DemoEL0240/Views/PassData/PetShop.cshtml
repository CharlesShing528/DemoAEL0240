@*明確指定 Model 型別，所以 View 為強型別檢視*@
@model List<string>

@{
    ViewData["Title"] = "PetShop";
}

<div class="jumbotron bg-info">
    <h2>PetShop寵物店</h2>
</div>


公司名稱: @ViewData["Company"]
<br />
公司地址: @ViewBag.Address
<br />

販賣的寵物有:
<br />
<ul>
    @foreach (var pet in Model)
    {
        <li>@pet</li>
    }

    @*@for(int i=0; i < Model.Count;i++)
        {
            <li>@Model[i]</li>
        }*@
</ul>
